{{#if hasBlock}}
  {{yield}}
{{else}}
  {{#each (sort-by (concat group.blockSortKey ':' group.blockSortDirection) rows) as |item|}}
    {{#if (hash-contains item group.blockQuery)}}
      <tr>
        {{#each-keys object=item as | key value|}}
          {{#unless (is-private-key key)}}
            <td>{{html-safe value}}</td>
          {{/unless}}
        {{/each-keys}}
      </tr>
      {{/if}}
  {{/each}}
{{/if}}
